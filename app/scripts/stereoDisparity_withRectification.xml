<application>
<name>Stereo Disparity Depth and Rectification</name>
<dependencies>
      <port>/iKinGazeCtrl/head/x:o</port>
      <port>/iKinGazeCtrl/head/q:o</port>
</dependencies>

<module>
      <name>stereoDisparity</name>
      <parameters>--from icubEyes.ini</parameters>
	  <node>icub-b15</node>
          <!--stdio>icub14</stdio-->
	<tag>stereoDisparity</tag>
   </module>
   
   <module>
      <name>yarpview</name>
      <parameters>--name /viewDisp --synch</parameters>
      <node>icub14</node>
	<tag>view1</tag>
</module>

<module>
      <name>yarpview</name>
      <parameters>--name /leftRect--synch</parameters>
      <node>icub14</node>
	<tag>view1</tag>
</module>

<module>
      <name>yarpview</name>
      <parameters>--name /rightRect --synch</parameters>
      <node>icub14</node>
	<tag>view1</tag>
</module>

<connection>
  <output>/stereoDisparity/disparity:o</output>
  <input>/viewDisp </input>
  <protocol>tcp</protocol>
</connection>

<connection>
  <output>/icub/camcalib/right/out</output>
  <input>/stereoDisparity/cam/right:i</input>
  <protocol>tcp</protocol>
</connection>

<connection>
  <output>/icub/camcalib/left/out</output>
  <input>/stereoDisparity/cam/left:i</input>
  <protocol>tcp</protocol>
</connection>


<connection>
 
  <output>/stereoDisparity/leftRect:o</output>
  <input>/leftRect</input>  
  <protocol>tcp</protocol>
</connection>

<connection>
 
  <output>/stereoDisparity/rightRect:o</output>
  <input>/rightRect</input>  
  <protocol>tcp</protocol>
</connection>
</application>
